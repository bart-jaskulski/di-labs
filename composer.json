{
  "name": "cleanweb/post-exporter",
    "description": "description",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": [
      "wordpress-plugin"
    ],
    "authors": [
    {
      "name": "Bartek Jaskulski",
      "email": "bjaskulski@protonmail.com"
    }
    ],
    "require": {
      "php": "^7.2 || ^8",
      "psr/log": "^1"
    },
    "require-dev": {
      "league/csv": "^9.6"
    },
    "config": {
      "platform": {
        "php": "7.2.5"
      }
    },
    "autoload": {
      "psr-4": {
        "CleanWeb\\PostExporter\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "CleanWeb\\PostExporter\\Tests\\": "tests"
      }
    },
    "scripts": {
      "test": "./tools/phpunit ./tests",
      "prefix": "./tools/php-scoper add-prefix --force"
    }
}
